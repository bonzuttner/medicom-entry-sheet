{
  "name": "pharmapop-entry-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "dev:all": "sh -c 'npm run typecheck:watch & TSC_PID=$!; npm run dev; kill $TSC_PID'",
    "dev:api": "vercel dev",
    "dev:api:all": "sh -c 'npm run typecheck:watch & TSC_PID=$!; npm run dev:api; kill $TSC_PID'",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vercel/blob": "^1.1.1",
    "@vercel/postgres": "^0.10.0",
    "jszip": "^3.10.1",
    "react-dom": "^19.2.4",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
